
citHeader("To cite exdyn in publications as:")

citEntry(
  entry="Manual",
  title = "exdyn: Testing and Simulating Explosive Periods",
  author       = personList(as.person("Kostas Vasilopoulos"),
                   as.person("Themis Pavlidis"),
                   as.person("Simon Spavound")),
  year = "2018",
  note = "R package version 0.2.0",

  textVersion  =
  paste("Vasilopoulos Kostas, Pavlidids Eftymios and Spavound Simon (2018).",
        "exdyn: Testing and Simulating Explosive Periods")
)
